{
    "name": "leewillis77/setting-store",
    "description": "Laravel 6+ package to provide a simple model / facade for key/value storage.",
    "keywords": [
        "Laravel",
	    "settings"
    ],
    "homepage": "https://github.com/leewillis77/setting-store",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Lee Willis",
            "homepage": "https://www.leewillis.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "laravel/framework": "~6.0|~7.0|~8.0"
    },
    "autoload": {
        "psr-4": {
            "Leewillis77\\SettingStore\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leewillis77\\SettingStore\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "SettingStore": "Leewillis77\\SettingStore\\SettingStoreFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2"
    }
}
